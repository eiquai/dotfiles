:call IMAP('CAS',  '\autocite[<+page+]{<+source+>}<++>', 'tex')
:call IMAP('CAL',  '\autocite[vgl.][<+page+]{<+source+>}<++>', 'tex')

:call IMAP('ITM',   '\item <++>', 'tex')
:call IMAP('ITD',   '\item [ <+label+> ] <++>', 'tex')

:call IMAP('MRR',  '\marginpar{<+side note+>}<++>', 'tex')

:call IMAP('FTN',  '\footnote{<+footnote+>}<++>', 'tex')

:call IMAP('TSK',  '\todo{<+task+>} <++>', 'tex')
:call IMAP('TLS',  '\tasklist <++>', 'tex')
